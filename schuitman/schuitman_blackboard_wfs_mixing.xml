<blackboardsystem>

    <dataConnection Type="AuditoryFrontEndKS">
       <Param Type="double">44100</Param>
    </dataConnection>

    <KS Name="seg" Type="StreamSegregationKS">
        <Param Type="char">~/projects/twoears/software/segmentation-training-pipeline/test/ExampleParameters.yaml</Param>
        <Param Type="double">2</Param>
        <Param Type="double">0</Param>
    </KS>
    
